{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "array-type": [true, "generic"],
        "max-line-length": [true, 256],
        "no-console": [false],
        "quotemark": [true, "single", "backtick"]
    },
    "rulesDirectory": []
}